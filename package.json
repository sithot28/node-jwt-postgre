{
  "name": "node-jwt-postgre",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint ",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/express-validator": "^3.0.0",
    "@types/helmet": "0.0.44",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/pg": "^7.11.1",
    "async": "^3.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^4.1.2",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "format": "^0.2.2",
    "helmet": "^3.21.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "lusca": "^1.6.1",
    "nodemailer": "^6.2.1",
    "nodemon": "^1.19.2",
    "pg": "^7.12.1",
    "pg-native": "^3.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "typescript": "^3.6.3",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^4.1.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1"
  }
}
